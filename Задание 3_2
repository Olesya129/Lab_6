public class laba {
    public static void main(String[] args) {
        int product = 1; int sum = 0;
        for (int i = 1; i <= 8; i++){
            for (int j = 1; j <= 3; j++){
                product *=  (i + j);
            }
            sum += product;
            product = 1;
        }
        System.out.println(sum);
    }
}
